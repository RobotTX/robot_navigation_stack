<?xml version="1.0"?>
<launch>
    <node pkg="map_server" type="map_server" name="map_server" args="$(find gobot_navigation)/maps/used_map.yaml" required="true">
        <param name="frame_id" type="string" value="/map" />
    </node>

    <node pkg="amcl" type="amcl" name="amcl" output="screen" respawn="true">
        <rosparam file="$(find gobot_navigation)/params/amcl_params.yaml" command="load" />
    </node>
</launch>
