<?xml version="1.0"?>
<launch>
    
    <node pkg="hector_exploration_node" type="exploration_planner_node" name="hector_exploration_node" output="screen">
        <rosparam file="$(find gobot_navigation)/params/costmap_common_params.yaml" command="load" ns="global_costmap" />
        <rosparam file="$(find gobot_navigation)/params/scan_global_costmap_params.yaml" command="load" />
    </node>

    <node pkg="hector_exploration_node" type="move_base_controller" name="hector_to_move_base" output="screen" required="true"/>

</launch>