<?xml version="1.0"?>
<launch>
	
	<include file="$(find gobot_software)/launch/robot_infos_params.launch"/>
	<param name="simulation" type="bool" value="true" />
	<node pkg="gobot_software" type="check_servers" name="software_check_servers" output="screen" required="true" />
	<node pkg="gobot_software" type="ping_servers" name="software_ping_servers" output="screen" required="true" />
	<node pkg="gobot_software" type="command_system" name ="software_command_system" output="screen" required="true"/>	
	<node pkg="gobot_software" type="robot_pos_transfer" name ="software_robot_pos_transfer" output="screen" required="true"/>
	<node pkg="gobot_software" type="map_transfer" name ="software_map_transfer" output="screen" required="true"/>
	<node pkg="gobot_software" type="read_new_map" name ="software_read_new_map" output="screen" required="true"/>
	<node pkg="gobot_software" type="teleop" name ="software_teleop" output="screen" required="true"/>
	<node pkg="gobot_software" type="gobot_status" name="software_gobot_status" output="screen" required="true" />
	<!-- node pkg="gobot_software" type="send_laser_data" name="software_send_laser_data" output="screen" required="true"/ -->
</launch>
