<?xml version="1.0"?>
<launch>
    <!-- MD49 AND STM32 RELATED -->
    <param name="wheel_separation" type="double" value="0.35" />
    <param name="wheel_radius" type="double" value="0.075" />
    <param name="ticks_per_rotation" type="double" value="982" />
    <param name="collision_threshold" type="double" value="4.0" />
    <param name="avoid_spd" type="int" value="12" />
    <param name="STMdevice" type="string" value="/dev/STM" />
    <param name="MD49device" type="string" value="/dev/MD" />
    <param name="STM_RATE" type="int" value="4" />
    <param name="ODOM_RATE" type="int" value="20" />
    <param name="STM_NEW" type="double" value="false" />
    <!-- RPLIDAR RELATED -->
    <param name="serial_port" type="string" value="/dev/rplidar"/>  
    <param name="serial_baudrate" type="int" value="115200"/>
    <param name="frame_id" type="string" value="base_laser"/>
    <param name="inverted" type="bool" value="false"/>
    <param name="angle_compensate" type="bool" value="true"/>
    <param name="angle_min" type="double" value="-95.0" />
    <param name="angle_max" type="double" value="105.0" />
    <!--SENSORS ON/OFF -->
    <param name="USE_BUMPER" type="bool" value="true" />
    <param name="USE_SONAR" type="bool" value="true" />
    <param name="USE_CLIFF" type="bool" value="true" />
    <!--BUMPER CLOUD RELATED -->
    <param name="dimension_x" type="double" value="0.255" />
    <param name="dimension_y" type="double" value="0.25" />
    <param name="bumpers_height" type="double" value="0.11" />
    <param name="pc_frame" type="string" value="base_link" />
    <!-- SONAR CLOUD RELATED -->
    <param name="SONAR_THRESHOLD" type="double" value="0.6" />
    <param name="SONAR_OUTRANGE" type="double" value="0.0" />
    <param name="SONAR_MAX" type="double" value="1.5" />
    <param name="SONAR_VIEW" type="double" value="0.15" />
    <param name="SONAR_RESOLUTION" type="double" value="50.0" />
    <!-- CLIFF CLOUD RELATED -->
    <param name="CLIFF_THRESHOLD" type="double" value="170.0" />
    <param name="CLIFF_OUTRANGE" type="double" value="0.0" />
</launch>
