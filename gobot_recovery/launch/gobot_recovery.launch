<?xml version="1.0"?>
<launch>     
    <param name="home_pose_file" type="string" value="$(find robot_infos)/Robot_Infos/home_pose.txt"/>
    <param name="last_pose_file" type="string" value="$(find robot_infos)/Robot_Infos/last_pose.txt"/>
    <param name="restart_robot_file" type="string" value="$(find robot_infos)/Robot_Infos/restart_robot.sh"/>

    <node pkg="gobot_recovery" type="allow_teb_back" name="allow_teb_back" output="screen" respawn="true"/>

    <node pkg="gobot_recovery" type="initialpose_estimation" name="initialpose_estimation" output="screen"/> #launch-prefix="xterm -e"/>

    <node pkg="gobot_recovery" type="startup" name="startup" output="screen"/>

</launch>